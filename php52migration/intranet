	<IfModule mod_ssl.c>
	        SSLEngine On
	        SSLProtocol -ALL +SSLv3 +TLSv1
	        SSLCipherSuite ALL:!aNULL:!ADH:!eNULL:!LOW:!EXP:RC4+RSA:+HIGH:!MEDIUM
			#SSLCertificateFile /***REMOVED***/secure/certificates/intranet_***REMOVED***_com.crt
			#SSLCertificateKeyFile /***REMOVED***/secure/certificates/intranet_***REMOVED***_com.key
			#SSLCertificateChainFile /***REMOVED***/secure/certificates/intranet_***REMOVED***_com.ca-bundle
			SSLCertificateFile /***REMOVED***/secure/certificates/wildcard-***REMOVED***.pem
			SSLCertificateKeyFile /***REMOVED***/secure/certificates/wildcard-***REMOVED***.key
			SSLCertificateChainFile /***REMOVED***/secure/certificates/wildcard-***REMOVED***-ca-bundle.crt

                <Files ~ "\.(***REMOVED***i|shtml|phtml|php?)$">
                        SSLOptions +StdEnvVars
                </Files>
                <Directory "/usr/lib/***REMOVED***i-bin">
                        SSLOptions +StdEnvVars
                </Directory>
                SetEnvIf User-Agent ".*MSIE.*" \
                        nokeepalive ssl-unclean-shutdown \
                        downgrade-1.0 force-response-1.0
	</IfModule>

	<IfModule mod_gnutls.c>
		GnuTLSEnable On
		GnuTLSSessionTickets On
		GnuTLSExportCertificates on
		GnuTLSPriorities SECURE
		#GnuTLSCertificateFile /***REMOVED***/secure/certificates/intranet_***REMOVED***_com.crt
		#GnuTLSKeyFile /***REMOVED***/secure/certificates/intranet_***REMOVED***_com.key
